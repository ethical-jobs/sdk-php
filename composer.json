{
  "name": "ethical-jobs/sdk-php",
  "type": "library",
  "description": "EthicalJobs API client and SDK",
  "homepage": "https://github.com/ethical-jobs/sdk-php",
  "keywords": [
    "ethicaljobs",
    "sdk"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "EthicalJobs.com.au",
      "homepage": "http://ethicaljobs.com.au"
    },
    {
      "name": "Andrew McLagan",
      "email": "andrew@ethicaljobs.com.au"
    }
  ],
  "require": {
    "php": "^7.2",
    "guzzlehttp/guzzle": "~6.0",
    "ethical-jobs/laravel-storage": "~1.6",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "illuminate/support": "^5.4"
  },
  "autoload": {
    "psr-4": {
      "EthicalJobs\\SDK\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "dont-discover": [
        "ethical-jobs/ethical-jobs-foundation"
      ],
      "providers": [
        "EthicalJobs\\SDK\\Laravel\\ServiceProvider"
      ],
      "aliases": {
        "EthicalJobs": "EthicalJobs\\SDK\\Laravel\\ApiFacade"
      }
    }
  }
}
